<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<!--parallel="methods"/"false"-->
<suite name="Test Android user login" parallel="false" thread-count="1" verbose="10">
    <test name="TC01">
        <classes>
            <class name="com.viewQwest.app.scripts.SHDROID03_LoginWithEmptyPasswordValidUsername"/>
        </classes>
    </test>
    <test name="TC02">
        <classes>
            <class name="com.viewQwest.app.scripts.SHDROID04_LoginWithEmptyValues"/>
        </classes>
    </test>
    <test name="TC03">
        <classes>
            <class name="com.viewQwest.app.scripts.SHDROID05_LoginWithInvalidEmailFormat"/>
        </classes>
    </test>
    <test name="TC04">
        <classes>
            <class name="com.viewQwest.app.scripts.SHDROID06_LoginWithInvalidPasswordValidUsername"/>
        </classes>
    </test>
    <test name="TC05">
        <classes>
            <class name="com.viewQwest.app.scripts.SHDROID07_LoginWithInvalidUsernameValidPassword"/>
        </classes>
    </test>
    <test name="TC06">
        <classes>
            <class name="com.viewQwest.app.scripts.SHDROID08_LoginWithSpecialCharacters"/>
        </classes>
    </test>
    <test name="TC07">
        <classes>
            <class name="com.viewQwest.app.scripts.SHDROID09_LoginWithValidUsernamePassword"/>
        </classes>
    </test>
</suite>
